function removeAbortCourseErr(){let t=$(".abort"),e=$("tr.registered"),r=$("tr.pending"),l=$("tr.is-theory"),o=r.length+e.length+l.length;if(t.length==o)return void alert("Không phát hiện thấy lỗi");let n=[],i=[];for(let e=0;e<t.length;e++){let r=t[e].getAttribute("data-rowindex");i.push(parseInt(r))}for(let t=0;t<o;t++){let e=t+1;i.includes(e)||n.push(e)}for(let t=0;t<n.length;t++)console.log(`javascript: Abort(${n[t]})`),Abort(n[t]);alert("Đợi vài giây thì bấm tải lại trang xem môn đã được xoá chưa nhé!")}try{removeAbortCourseErr()}catch(t){console.log("Lỗi! Hãy thử lại")}